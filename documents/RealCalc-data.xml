  <group name="Time">
    <unit name="Nanoseconds"           symbol="ns"          ratio="1.0e-9"                  />
    <unit name="Microseconds"          symbol="µs"          ratio="1.0e-6"                  />
    <unit name="Milliseconds"          symbol="ms"          ratio="0.001"                   />
